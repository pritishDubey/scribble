div
  .row.searchBox
    .col.s8
      .searchWrapper
        input(type = 'text' placeholder = 'Search')
    .col.s4
      .row
        .col.s6.iconContainer
          center
            i.fa.fa-floppy-o.fa-lg.iconBox(ng-click = 'save()')
        .col.s6.iconContainer
          center
            i.fa.fa-trash-o.fa-lg.iconBox(ng-click = 'delete()')
  .row.listBoxWrapper
    .listBox
      .collection
        a.collection-item(href='',ng-repeat = "document in documents | orderBy:'-'" )
          span
            b {{document.subject}}
          br
          span {{document.date}}
